@page "/data/mypersonmanager/savedurl/delete/{UrlId:int}"

@inherits SavedUrlBasePageClass

<PageTitle>Delete @SavedUrl.Title</PageTitle>

<h3>Delete</h3>

<h4 style="color: red">Are you sure you want to delete this saved url?</h4>

<SavedUrlDetails SavedUrl="SavedUrl" />

<button class="btn btn-danger" @onclick="DeleteSavedUrl">Delete</button> | <a href="/data/mypersonmanager/savedurls/index">Cancel</a>
